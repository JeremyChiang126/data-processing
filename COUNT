def get_counts(sequence):
  counts={}
  for x in sequence:
    if x in counts:
      counts[x] += 1
     else:
      counts[x] = 1
   return counts
   

#标准库
from collections import defaultdict

def get_counts2(sequence):
  counts = defultdict(int)
  for x in sequence:
    counts[x] += 1
  return counts
    
